{
  "name": "gitlab-runner",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev src/local.ts",
    "watch": "ts-node-dev --respawn --exit-child src/local.ts",
    "build": "tsc",
    "dist": "yarn build && cd build && cp ../package.json . && yarn --production && zip -r  ../webhook.zip .",
    "test": "NODE_ENV=test jest",
    "clean:build": "rimraf build",
    "clean:all": "rimraf build node_modules",
    "lint": "echo Add linting here...",
    "cli": "yarn build && ./bin/parse.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.5",
    "aws-sdk": "^2.645.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
}
