{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ssm:DeleteParameter"
      ],
      "Resource": [
        %{~ for arn in arns ~}
        "${arn}"
        %{~ endfor ~}
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "ssm:GetParameters",
        "ssm:GetParameter"
      ],
      "Resource": [
        %{~ for arn in arns ~}
        "${arn}"
        %{~ endfor ~}
      ]
    }
  ]
}
